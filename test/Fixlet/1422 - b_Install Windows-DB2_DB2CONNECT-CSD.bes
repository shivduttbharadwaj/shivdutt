<?xml version="1.0" encoding="UTF-8"?>
<BES xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="BES.xsd">
	<Fixlet>
		<Title>_Install Windows-DB2_DB2CONNECT-CSD</Title>
		<Description><![CDATA[This fixlet will install the endpoint code for Windows-DB2_DB2CONNECT-CSD<BR/><BR/><B>CSD Version Supported:&nbsp;</B>4.0D
		<p>Endpoints must have at least 9 MB available on the file system where the BES client is located for this fixlet to become applicable.</p>
		<p><small><b>Version:</b>&nbsp;4.0.0<br><b>Build ID:</b>&nbsp;20190307-2125</small></p><P>----------------------------------------------------------------------------------<BR>Licensed Materials - Property of IBM<BR>(c) Copyright IBM Corp. 2019. All Rights Reserved.</P>
		]]></Description>
		<Relevance>
				
		(
			if exists setting "_GCM_Windows-DB2_DB2CONNECT-CSD_OSRelevance_Override" whose (value of it != "") of client then (
				true
			)
			else (
				disjunction of ((name of operating system as lowercase contains it) of ( "win2008"; "win2012"; "win2016"))
			)
		)
		</Relevance>
		<Relevance>
				
		(
			if exists setting "_GCM_Windows-DB2_DB2CONNECT-CSD_MWRelevance_Override" whose (value of it != "") of client then (
				true
			)
			else (
				(
					(exists key "HKLM\SOFTWARE\IBM\DB2" of x32 registry|false) or (exists key "HKLM\SOFTWARE\IBM\DB2" of x64 registry|false)

				) 
					or (if (exists file "subs.all.sss" of folder "__GTS/SSS" of storage folder of client | false) then
		(exists (lines containing "SUBSYSTEM_TYPE=DB2" of file "subs.all.sss" of folder "__GTS/SSS" of storage folder of client) whose (not (it is "" or it contains "SUBSYSTEM_INSTANCE=NO")) | false)
		else false)

			)
		)		
		</Relevance>
		<Relevance><![CDATA[(version of client >= "9.2")]]></Relevance>
		<Relevance>exists file "HCLauncher_400/HCLauncher.jar" of folder "__GTS" of storage folder of client </Relevance>
		<Relevance>(exists file "jre/bin/java.exe" of folder "__GTS" of storage folder of client or exists file "jre/bin/java" of folder "__GTS" of storage folder of client)</Relevance>
		<Relevance><![CDATA[not exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client]]></Relevance>
		<Relevance><![CDATA[
		(if name of operating system as lowercase contains "win" then
(
	(free space of drive of storage folder of client)
)
else 
(
	if name of operating system as lowercase contains "sunos" then
	(
		 ((free space of filesystem of storage folder of client) - (total space of filesystem of storage folder of client / 100))
	)
	else	
	(
		(free space of filesystem of storage folder of client)
	)
)
) > 9937485
		]]></Relevance>
		<Category>Middleware</Category>
		<Source>GCM</Source>
		<SourceID>4.0.0</SourceID>
		<SourceReleaseDate>2019-03-07</SourceReleaseDate>
		<MIMEField>
			<Name>x-fixlet-ibm-copyright</Name>
			<Value>Licensed Materials - Property of IBM; (c) Copyright IBM Corp. 2019. All Rights Reserved.</Value>
		</MIMEField>
		<MIMEField>
			<Name>x-gcm-author</Name>
			<Value>GTS GCM Team</Value>
		</MIMEField>
		<MIMEField>
			<Name>x-gcm-origin</Name>
			<Value>GTS GCM Team</Value>
		</MIMEField>
		<MIMEField>
			<Name>x-supported-csd-version</Name>
			<Value>4.0D</Value>
		</MIMEField>
		<MIMEField>
			<Name>x-fixlet-requested-id</Name>
			<Value>10790002</Value>
		</MIMEField>
		<MIMEField>
			<Name>x-gcm-purpose</Name>
			<Value>lifecycle</Value>
		</MIMEField>
		<MIMEField>
			<Name>x-fixlet-modification-time</Name>
			<Value>Wed, 27 Mar 2019 09:02:51 +0000</Value>
		</MIMEField>
		<Domain>BESC</Domain>
		<DefaultAction ID="Action1">
			<Description>
				<PreLink>Click </PreLink>
				<Link>here</Link>
				<PostLink> to deploy this action.</PostLink>
			</Description>
			<ActionScript MIMEType="application/x-Fixlet-Windows-Shell"><![CDATA[
			// Licensed Materials - Property of IBM; (c) Copyright IBM Corporation 2019. All Rights Reserved.
			begin prefetch block	
	if {exists setting "_GCM_Download_BasePath" whose (value of it != "") of client}
		parameter "basepath" = "{value of setting "_GCM_Download_BasePath" of client}"
	else
		parameter "basepath" = "GTSProtocol://download.bigfix.com/download/gtscontent/iemhc"
	endif
	add prefetch item name=policy.xml sha1=c153ad4be71128522166bc824f3c38d58ba6f385 sha256=07c2e677d87d97ee9830ea5a20664984834adc0d86bbebeedf198771dc2b90d7 size=142623 url={parameter "basepath"}/400/20190307-2125/Windows/DB2.DB2CONNECT.Windows.CSD/policy.xml
	add prefetch item name=Collectors.zip sha1=1ae7d5c2ebbb07277ec96720bf79073564a03214 sha256=831cfab7ed5542b2c4672b9b9cb76f6c2f0809aacd4ea65022c2860a43d96310 size=3169872 url={parameter "basepath"}/400/20190307-2125/Windows/DB2.DB2CONNECT.Windows.CSD/Collectors.zip
	add prefetch item name=cuz.jar sha1=76dd952a93b4bbda50511acff681d6805487baa7 sha256=ce2147227f5822c3e9a4e7909bf4178ea8299de3d6e9c118e813c85e53492caa size=179698 url={parameter "basepath"}/400/20190307-2125/tools/cuz.jar
end prefetch block

parameter "build_id" = "20190307-2125"
parameter "version" = "4.0.0"

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client}
	folder create "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD_backup"
endif

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client AND exists file "policy_parameters.txt" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD/policy_parameters.txt" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD_backup/policy_parameters.txt"
endif

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client AND exists file "localpolicy.xml" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD/localpolicy.xml" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD_backup/localpolicy.xml"
endif

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client AND exists file "freshness.txt" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD/freshness.txt" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD_backup/freshness.txt"
endif

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client AND exists folder "vault" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD/vault" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD_backup/vault"
endif
if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows-DB2_DB2CONNECT-CSD") of storage folder of client}
	folder delete "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD"
endif

folder create "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD"

copy "{client folder of current site}/__Download/policy.xml" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD/policy.xml"

// unzip the Collectors.zip
if {windows of operating system} 
	wait "{storage folder of client}\__GTS\jre\bin\java.exe" -jar "{client folder of current site}\__Download\cuz.jar" "{storage folder of client}\__GTS\__IEMHC\{name of current site}\Windows-DB2_DB2CONNECT-CSD" "{client folder of current site}\__Download\Collectors.zip" 	
else
	wait "{storage folder of client}/__GTS/jre/bin/java" -jar "{client folder of current site}/__Download/cuz.jar" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD" "{client folder of current site}/__Download/Collectors.zip"	
endif


if {exit code of action != 0}
exit {exit code of action}
endif

// create version.txt
appendfile {now as string}
appendfile DB2-DB2CONNECT-Windows-CSD-4.0D
appendfile {parameter "version"}
appendfile {parameter "build_id"}
delete "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD/version.txt"
move __appendfile "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows-DB2_DB2CONNECT-CSD/version.txt"

// Set the GTS_GCM_Enhanched_Scheduling_<site name> setting to DISABLED if the setting is empty or does not exists AND there is no GTS_<site name>_GCM_Dates setting
if {( (not exists setting ("GTS_GCM_ENHANCED_SCHEDULING_"&(name of current site)) whose (value of it != "") of client) AND ((not exists values of setting ("GTS_"&(name of current site)&"_GCM_Dates") of client)|True) )}
	setting "GTS_GCM_ENHANCED_SCHEDULING_{name of current site}"="DISABLED" on "{date (local time zone) of now & time (local time zone) of now}" for client
endif
			]]></ActionScript>
		</DefaultAction>
	</Fixlet>
</BES>
