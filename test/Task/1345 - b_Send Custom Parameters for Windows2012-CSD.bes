		]]></Description>
		<Relevance>
				
		(
			if exists setting "_GCM_Windows2012-CSD_OSRelevance_Override" whose (value of it != "") of client then (
				true
			)
			else (
				name of operating system as lowercase contains "win2012"
			)
		)
		</Relevance>
