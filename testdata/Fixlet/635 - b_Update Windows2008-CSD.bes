			</Description>
			<ActionScript MIMEType="application/x-Fixlet-Windows-Shell"><![CDATA[
			// Licensed Materials - Property of IBM; (c) Copyright IBM Corporation 2019. All Rights Reserved.
			begin prefetch block	
	if {exists setting "_GCM_Download_BasePath" whose (value of it != "") of client}
		parameter "basepath" = "{value of setting "_GCM_Download_BasePath" of client}"
	else
		parameter "basepath" = "GTSProtocol://download.bigfix.com/download/gtscontent/iemhc"
	endif
	add prefetch item name=policy.xml sha1=9eea655378b7bac02ceed5a96c01d2013762c7d2 sha256=5544f7b99c983453c8d3d0b146aae5082e6a2c7f6112e1b229ca8009c5a2102e size=338448 url={parameter "basepath"}/390/20190123-2090/Windows/Windows.2008.CSD/policy.xml
	add prefetch item name=Collectors.zip sha1=3ec105b3337a6b4d05521ec6491116297dcde55a sha256=f7fbf61668dedb2b1c65ab877f6235a8526981b125f4758fe2c730fa067c9593 size=6223890 url={parameter "basepath"}/390/20190123-2090/Windows/Windows.2008.CSD/Collectors.zip
	add prefetch item name=cuz.jar sha1=76dd952a93b4bbda50511acff681d6805487baa7 sha256=ce2147227f5822c3e9a4e7909bf4178ea8299de3d6e9c118e813c85e53492caa size=179698 url={parameter "basepath"}/390/20190123-2090/tools/cuz.jar
	add prefetch item name=RemediationScript.zip sha1=d499657aed4540fa7568ddcc207cabe4de46e004 sha256=28ae7e992b8c39fd60d2886bc0c5fb392a1ee7d0b5ea6aca9e26137d47812348 size=169521 url={parameter "basepath"}/390/20190123-2090/Windows/Windows.2008.CSD/RemediationScript.zip
end prefetch block

parameter "build_id" = "20190123-2090"
parameter "version" = "3.9.0"

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client}
	folder create "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup"
endif

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client AND exists file "policy_parameters.txt" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/policy_parameters.txt" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup/policy_parameters.txt"
endif

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client AND exists file "localpolicy.xml" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/localpolicy.xml" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup/localpolicy.xml"
endif

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client AND exists file "freshness.txt" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/freshness.txt" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup/freshness.txt"
endif

if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client AND exists folder "vault" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/vault" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup/vault"
endif
if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client}
	folder delete "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD"
endif

folder create "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD"

copy "{client folder of current site}/__Download/policy.xml" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/policy.xml"

// unzip the Collectors.zip
if {windows of operating system} 
	wait "{storage folder of client}\__GTS\jre\bin\java.exe" -jar "{client folder of current site}\__Download\cuz.jar" "{storage folder of client}\__GTS\__IEMHC\{name of current site}\Windows2008-CSD" "{client folder of current site}\__Download\Collectors.zip" 	
else
	wait "{storage folder of client}/__GTS/jre/bin/java" -jar "{client folder of current site}/__Download/cuz.jar" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD" "{client folder of current site}/__Download/Collectors.zip"	
endif

// unzip the RemediationScript.zip
if {windows of operating system} 
	wait "{storage folder of client}\__GTS\jre\bin\java.exe" -jar "{client folder of current site}\__Download\cuz.jar" "{storage folder of client}\__GTS\__IEMHC\{name of current site}\Windows2008-CSD" "{client folder of current site}\__Download\RemediationScript.zip" RemediationScript
else
	wait "{storage folder of client}/__GTS/jre/bin/java" -jar "{client folder of current site}/__Download/cuz.jar" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD" "{client folder of current site}/__Download/RemediationScript.zip" RemediationScript
endif

if {exit code of action != 0}
exit {exit code of action}
endif

// create version.txt
appendfile {now as string}
appendfile Windows-2008-CSD-4.0C
appendfile {parameter "version"}
appendfile {parameter "build_id"}
delete "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/version.txt"
move __appendfile "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/version.txt"

// Set the GTS_GCM_Enhanched_Scheduling_<site name> setting to DISABLED if the setting is empty or does not exists AND there is no GTS_<site name>_GCM_Dates setting
if {( (not exists setting ("GTS_GCM_ENHANCED_SCHEDULING_"&(name of current site)) whose (value of it != "") of client) AND ((not exists values of setting ("GTS_"&(name of current site)&"_GCM_Dates") of client)|True) )}
	setting "GTS_GCM_ENHANCED_SCHEDULING_{name of current site}"="DISABLED" on "{date (local time zone) of now & time (local time zone) of now}" for client
endif			
						if {exists file "policy_parameters.txt" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD_backup") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup/policy_parameters.txt" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/policy_parameters.txt"
endif

if {exists file "localpolicy.xml" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD_backup") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup/localpolicy.xml" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/localpolicy.xml"
endif

if {exists file "freshness.txt" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD_backup") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup/freshness.txt" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/freshness.txt"
endif

if {exists folder "vault" of folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD_backup") of storage folder of client}
	move "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup/vault" "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD/vault"
endif

folder delete "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD_backup"
			]]></ActionScript>
