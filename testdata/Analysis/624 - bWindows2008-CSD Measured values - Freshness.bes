			<Value>         {  "fusion_supported_platforms" : "Windows Server 2008",  "description" : "Freshness",  "scm_parameters" : {   "properties" : {    "1" : {     "type" : "string",     "description" : "Result file modification date",     "order" : 1,     "title" : "Last Update"    },    "2" : {     "type" : "string",     "description" : "Result file freshness status",     "order" : 1,     "title" : "Status"    }   }  },  "title" : "Windows2008-CSD Measured values - Freshness" }     </Value>
