wait msiexec.exe /p "__Download\Acrobat2015Upd1500630121.msp" REINSTALL=ALL REINSTALLMODE=omu /qn

action may require restart "0ed68b8fd911e5f8097ce327404b4f9a6f7929f3"</ActionScript>
