wait msiexec.exe /p "__Download\AcrobatDCUpd1900820080.msp" REINSTALL=ALL REINSTALLMODE=omu /qn

action may require restart "a1508f30a8a5a505d1c426668c28a82fe62d9f18"</ActionScript>
