appendfile REGEDIT4
appendfile [HKEY_LOCAL_MACHINE\SOFTWARE\Adobe\Acrobat Reader\11.0\AdobeViewer]
appendfile "EULA"=dword:00000001
appendfile "Launched"=dword:00000001
appendfile

