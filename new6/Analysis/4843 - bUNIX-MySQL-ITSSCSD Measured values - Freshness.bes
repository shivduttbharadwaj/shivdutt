		<Domain>BESC</Domain>
		<Property Name="Last Update" ID="1"><![CDATA[ 						( if ( exists file it) then if (line 1 of file it as trimmed string as lowercase = "unknown") then ("Never") 	else ( (line 1 of file it ) ) else ("Never")) of (storage folder of client as string & "/__GTS/__IEMHC/" & name of site of current analysis & "/" & 	"UNIX-MySQL-ITSSCSD" & "/delay_counter_" & "policy_result.xml")		  		]]></Property>
		<Property Name="Status" ID="2"><![CDATA[ 				(if (exists file it) then (if ((line 1 of file it)  as trimmed string as lowercase = "unknown") then "Foul" else if ((now - (line 1 of file it as time)) < ((if exists (file "freshness.txt" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/UNIX-MySQL-ITSSCSD") of storage folder of client) then ((line of file "freshness.txt" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/UNIX-MySQL-ITSSCSD") of storage folder of client) as integer) else 5) * day)) then "Fresh" else if ((now - (line 1 of file it as time)) = ((if exists (file "freshness.txt" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/UNIX-MySQL-ITSSCSD") of storage folder of client) then ((line of file "freshness.txt" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/UNIX-MySQL-ITSSCSD") of storage folder of client) as integer) else 5) * day)) then "Ripe" else "Rotten") else "Foul" ) of ( storage folder of client as string & "/__GTS/__IEMHC/" & name of site of current analysis & "/" & "UNIX-MySQL-ITSSCSD" & "/delay_counter_" & 	"policy_result.xml" ) 		]]></Property>
