}
ORS="";
alph="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/";
first4 = substr($0,1,4);
pos = 1;
n=1;
while (n>0) {{
	part = substr($0,pos,4);
	n = length(part);
	if (n == 0)
		break;
	arr[1] = substr($0,pos,1);
	pos++;
	arr[2] = substr($0,pos,1);
	pos++;
	arr[3] = substr($0,pos,1);
	pos++;
	arr[4] = substr($0,pos,1);
	pos++;
	quad = sprintf("%06d%06d%06d%06d", tobin(index(alph,arr[1])-1), tobin(index(alph,arr[2])-1), tobin(index(alph,arr[3])-1), tobin(index(alph,arr[4])-1));
	part1 = substr(quad,1,8);
	part2 = substr(quad,9,8);
	part3 = substr(quad,17,8);
	print chr(oct(part1)) > outfile;
	if (arr[3] != "=")
		print chr(oct(part2)) > outfile;
	if (arr[4] != "=")
		print chr(oct(part3)) > outfile;
	if (arr[4] == "=")
		break;
}
}
function chr(s) {{
	return sprintf("%c", s + 0);
}
function oct(s) {{
	tot=0;
	bins[1] = 128;
	bins[2] = 64;
	bins[3] = 32;
	bins[4] = 16;
	bins[5] = 8;
	bins[6] = 4;
	bins[7] = 2;
	bins[8] = 1;
	for (i=1; i<=8; i++) {{
		x = substr(s,i,1);
		tot+=x*bins[i];
	}
	return tot;
}
function tobin(innum) {{
	retstr = "";
	t=innum;
	while( t ) {{
		if ( t%2==0 ) {{
			retstr = "0" retstr;
		} else {{
			retstr = "1" retstr;
		}
		t = int(t/2);
	}

	return retstr;
}

