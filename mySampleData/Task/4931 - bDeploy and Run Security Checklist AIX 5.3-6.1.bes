// extractor.sh
//
createfile until __EOF
#!/bin/sh
# figure out the target directory for the scripts
#if [ "`uname -s`" == "Linux" ]
#then
#        T=SCM/`uname -s`/`cat /etc/redhat-release | awk '{{print $(NF-1)}'|awk -F. '{{print $1}'`
#else
#        T=SCM/`uname -s`/`uname -r`
#fi
OsName=`uname -s`
if [ $OsName = Linux ]
then
        if [ -f /etc/redhat-release ]; then
                if [ `egrep -ic "release 3" /etc/redhat-release` -gt 0 ]
                then
                        OsRelease=3
                elif [ `egrep -ic "release 4" /etc/redhat-release` -gt 0 ]
                then
                        OsRelease=4
                elif [ `egrep -ic "release 5" /etc/redhat-release` -gt 0 ]
                then
                        OsRelease=5
                elif [ `egrep -ic "release 6" /etc/redhat-release` -gt 0 ]
                then
                        OsRelease=6
                else
                        OsRelease=7
                fi
        elif [ -f /etc/SuSE-release ]; then
                RELEASENUM=`awk '/^VERSION =/{{print $3}' /etc/SuSE-release | awk -F\. '{{print $1}'`
                OsRelease="sles"$RELEASENUM
        fi
elif [ $OsName = AIX ]
then
	unamev=`uname -v`
	if [ $unamev -eq 6 ] || [ $unamev -eq 7 ]
	then
		OsRelease=$unamev`uname -r`
	else
                OsRelease=`uname -r`
        fi
elif [ $OsName = Darwin ]
then
        #OsRelease=`sw_vers -productVersion`
        OsRelease=`sw_vers -productVersion | awk -F\. '{{print $1 "." $2}'`
else
        OsRelease=`uname -r`
fi
T=SCM/$OsName/$OsRelease
#P="{((pathname of parent folder of parent folder of folder (pathname of client folder of current site)) & "/" & (name of current site)) & "/scm_preserve/" }"
P=scm_preserve
rm -f scm_preserve 2>/dev/null
mkdir -p "$T" "$P"
#ln -s "$P" .
# extract detect scripts
while read line; do
	echo "$line" | awk -F: '{{f=$1".64";print > f}'
done < detect_scripts0.txt
for chunkid in 1 2 3 4 5 6 7 8 9; do
	while read line; do
		echo "$line" | awk -F: '{{f=$1".64";print $2 >> f}'
	done < detect_scripts$chunkid.txt
done
for f in *.64; do
	cat $f | awk -f base64.decode -F:
done
rm *.64
mv *.detect "$T" >/dev/null

# extract remediation scripts if they exist
if [ -r remediation_scripts0.txt ]
then
        while read line; do
		echo "$line" | awk -F: '{{f=$1".64";print > f}'
        done < remediation_scripts0.txt
	for chunkid in 1 2 3 4 5 6 7 8 9; do
		while read line; do
			echo "$line" | awk -F: '{{f=$1".64";print $2 >> f}'
		done < remediation_scripts$chunkid.txt
	done
	for f in *.64; do
		cat $f | awk -f base64.decode -F:
	done
	rm *.64
	mv *.remediate "$T" >/dev/null
fi
globalfind=
if [ `grep -c yes needglobalfind.txt` -gt 0 ]
then
	globalfind='-g'
fi

# extract parameters
cp -f parameters.txt "$T"/params

# make link so remediate works for latest site that deploy and run ran in
rm -f ../actionsite/SCM
#ln -s "../CustomSite_{name of current site}/SCM" ../actionsite
ln -s "`pwd`/SCM" ../actionsite
for d in ../opsite*; do
	if [ ! -d "$d" ]
	then
		continue
	fi
	rm -f $d/SCM
	ln -s "`pwd`/SCM" $d
done
# run the scripts
cd SCM
__EOF
move __createfile extractor.sh
if { not exists true whose ( if true then ( exists value of setting "_SCM_EXTRACT_ONLY" of client ) else false ) }
delete runfile.txt
appendfile echo /bin/sh runme.sh \$globalfind >> extractor.sh
move __appendfile runfile.txt
wait /bin/sh runfile.txt
endif
wait /bin/sh extractor.sh
]]></ActionScript>
