		<DefaultAction ID="Action1">
			<Description>
				<PreLink>Click </PreLink>
				<Link>here</Link>
				<PostLink> to deploy this action.</PostLink>
			</Description>
			<ActionScript MIMEType="application/x-Fixlet-Windows-Shell"><![CDATA[
			// Licensed Materials - Property of IBM; (c) Copyright IBM Corporation 2019. All Rights Reserved.
			
if {exists folder ("__GTS/__IEMHC/" & name of current site as string & "/Windows2008-CSD") of storage folder of client}
	folder delete "{storage folder of client}/__GTS/__IEMHC/{name of current site}/Windows2008-CSD"
endif

if {exists folder (name of current site as string) of folder "__GTS/__IEMHC" of storage folder of client AND not exists descendant folders of folder ("__GTS/__IEMHC/"& name of current site as string ) of storage folder of client}
	folder delete "{storage folder of client}/__GTS/__IEMHC/{name of current site}"
endif
			]]></ActionScript>
		</DefaultAction>
	</Fixlet>
