		<Property Name="Health Check" ID="10"><![CDATA[ 		if (exists file "hcl_xout_0_0.log" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/Domino-CSD") of storage folder of client | False)  		then  			if (exists lines whose (it contains "[FATAL]" or it contains "[ERROR]") of file "hcl_xout_0_0.log" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/Domino-CSD") of storage folder of client)  			then (lines whose (it contains "[FATAL]" or it contains "[ERROR]" ) of file "hcl_xout_0_0.log" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/Domino-CSD") of storage folder of client) 			else ( 				if (exists file "policy_result.xml" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/Domino-CSD") of storage folder of client | False)  		 				then ( 			 					if (( exists (line of (file "policy_result.xml" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/Domino-CSD") of storage folder of client)) whose (it contains "ERROR !!! "))) 			 					then (following texts of firsts "<message>" of preceding texts of firsts"</message>" of substrings separated by "</violation>" of lines of (file "policy_result.xml" of folder ("__GTS/__IEMHC/" & name of site of current analysis & "/Domino-CSD") of storage folder of client)) whose (it contains "ERROR !!! ") 			 					else "Successful Health Check Execution."  				)		 				else "No Health Check Execution." 			)	 		else "No Health Check Execution."	 		]]></Property>
