]]></Description>
		<Relevance>(if( name of operating system starts with "Win" ) then platform id of operating system != 3 else false) AND (if exists property "in proxy agent context" then ( not in proxy agent context ) else true )</Relevance>
		<Relevance>(language of version block of file "kernel32.dll" of system folder contains "English") OR (exists key "HKLM\System\CurrentControlSet\Control\Nls\MUILanguages" whose (exists value of it) of registry)</Relevance>
		<Relevance>not exists values "PROCESSOR_ARCHITECTURE" whose (it as string as lowercase = "ia64") of keys "HKLM\SYSTEM\CurrentControlSet\Control\Session Manager\Environment" of registry</Relevance>
		<Relevance><![CDATA[((exists file "WINWORD.EXE" whose (version of it < "14.0.5123.5000") of it) OR (exists file "WORDICON.EXE" whose (version of it < "14.0.5120.5000") of it) OR (exists file "WRD12CNV.DLL" whose (version of it < "14.0.5123.5000") of it) OR (exists file "WWLIB.DLL" whose (version of it < "14.0.5123.5000") of it)) of folder (value "Path" of key "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Office\14.0\Common\InstallRoot" of registry as string)]]></Relevance>
		<Relevance><![CDATA[exists key whose ((it = "14" AND it >= "14.0.4763.1000") of (value "DisplayVersion" of it as string as version) AND exists value "DisplayName" of it AND (((length of it = 38) AND (it contains "000000FF1CE%7D") AND ((it = "0000" OR (hexadecimal integer it = 1033)) of last 4 of (first 19 of it)) AND ((it = "0011" OR it = "0012" OR it = "0013" OR it = "0014" OR it = "002F" OR it = "008B") of last 4 of (first 14 of it)) AND ((it = "0000") of last 4 of (first 24 of it))) of (name of it))) of key "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall" of registry]]></Relevance>
		<Relevance><![CDATA[exists regapp "winword.exe" whose (version of it = "14" AND version of it >= "14.0.4762.1000")]]></Relevance>
